#include<bits/stdc++.h>
using namespace std;
int main()
{
int t;
cin>>t;
while(t--)
{
int n,c1=0,d1=0;
cin>>n;
char c[n],d[n];
for(int i=0;i<n;i++)
{
cin>>c[i];
if(c[i]=='1')
c1++;
}
for(int i=0;i<n;i++)
{
cin>>d[i];
if(d[i]=='1')
d1++;
}
if(c1==d1)
{
	cout<<"YES"<<endl;
}
else
{
	cout<<"NO"<<endl;
}
}
}
